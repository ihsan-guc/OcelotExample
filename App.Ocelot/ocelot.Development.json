{
  "Routes": [
    {
      "DownstreamPathTemplate": "/user/GetUserList",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "appone",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/appone/user/userList",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "appone"
    },
    {
      "DownstreamPathTemplate": "/user/AddUser",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "appone",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/appone/user/AddUser",
      "UpstreamHttpMethod": [ "POST" ],
      "SwaggerKey": "appone"
    },
    {
      "DownstreamPathTemplate": "/user/GetUser",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "appone",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/appone/user/GetUser/{id}",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "appone"
    },
    {
      "DownstreamPathTemplate": "/WeatherForecast",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "apptwo",
          "Port": 80
        }
      ],
      "UpstreamPathTemplate": "/apptwo/WeatherForecast",
      "UpstreamHttpMethod": [ "GET" ],
      "SwaggerKey": "apptwo"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "appone",
      "Config": [
        {
          "Name": "App One Api",
          "Version": "v1",
          "Url": "http://appone/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "apptwo",
      "Config": [
        {
          "Name": "App Two Api",
          "Version": "v1",
          "Url": "http://apptwo/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://appocelot"
  }
}
